{
    "conditions": {
        "slab_edge_wall_intersection": {
            "where": {
                "elements_of_types": [
                    "elements.wall", "elements.floor"
                ],
                "condition": "touch",
                "execute": {
                    "function_id":"slab-setback",
                    "arguments": {
                        "setback_amount": 0.1
                    }
                }
            }
        }
    }
}